{
  "coc.preferences.useQuickfixForLocations": false,
  "diagnostic.level": "warning",
  "diagnostic.errorSign": ">>",
  "diagnostic.warningSign": "--",
  "suggest.autoTrigger": "trigger",
  "list.previewHighlightGroup": "Visual",
  "coc.preferences.messageLevel": "warning",
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.diagnostics.enableExperimental": false,
  "rust-analyzer.inlayHints.chainingHints": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enable": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript"],
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "disableDiagnostics": false,
      "rootPatterns": ["deps.edn", "project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
        "project-specs": [{
          "project-path": "deps.edn",
          "classpath-cmd": ["clj", "-Spath"]
        }],
        "use-metadata-for-privacy?": true,
        "ignore-classpath-directories": true
      }
    },
    "fsharp": {
      "shell": true,
      "command": "dotnet",
      "args": [
        "$HOME/release-fsautocomplete/fsautocomplete.dll",
        "--background-service-enabled"
      ],
      "filetypes": ["fsharp"],
      "initializationOptions": {
        "AutomaticWorkspaceInit": true
      },
      "ignoredRootPaths": ["/", "~"],
      "settings": {}
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
